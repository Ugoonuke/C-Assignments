#include <stdio.h>
#include <stdlib.h>

//Program 1b ------------------------------
#include <stdio.h>
int main ()
{
    float one = 9.0/4.0;
    float two = 17.0*3.0;

    printf("One = 9.0/4.0\n");
    printf("Two = 17.0/3.0\n");

    printf("Two divided by One is %.2f\n", one);
    printf("Two multiplied by One is %.2f\n", two);
return 0;
}
