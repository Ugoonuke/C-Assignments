/* ------------------------------
 Program: output.c
 Author: Ugo-Michael Onuke
 Purpose: Write a program that uses rand() to generate a set of 10 random numbers from a finite set 1 â€“ 100 and saves each one to an array random[10].
 ------------------------------*/
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    system("color F9");
    srand(time (NULL));
    for(int i=0; i<10; i++)
    {
        printf("%d\n", (1+( rand() % 100)));
    }

    return 0;
}
